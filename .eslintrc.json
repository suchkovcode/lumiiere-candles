{
   "root": true,
   "env": {
      "es6": true,
      "browser": true,
      "commonjs": true,
      "node": true
   },
   "extends": ["plugin:vue/vue3-essential"],
   "plugins": ["import"],
   "parserOptions": {
      "ecmaVersion": 13,
      "sourceType": "module"
   },

   "rules": {
      "vue/html-indent": ["error", 3]
   }
}
